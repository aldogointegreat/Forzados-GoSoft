	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev --turbopack -H 0.0.0.0 -p 3001",
		"start:ad": ".venv\\Scripts\\uvicorn.exe activeDirectory.conexion_py:app --reload --port 8000",
		"start:ad:win": ".venv\\Scripts\\uvicorn.exe activeDirectory.conexion_py:app --reload --port 8000",
		"dev:all": "concurrently \"npm run dev\" \"npm run start:ad\"",
		"build": "next build",
		"start": "next start -H 0.0.0.0 -p 3002",
		"start:all": "concurrently \"npm run start\" \"npm run start:ad\"",
		"lint": "next lint",
		"check": "pnpm lint && pnpm tsc --noEmit",
